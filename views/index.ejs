<%- include('./partials/head') %>
<%- include('./partials/header') %>

<section>
  <% persons.forEach(person => { %>
  <div>
    <div class="logo">
      <img src="<%= person.avatar %>" alt="foto">
    </div>
    <h2><%= person.name %></h2>
    <h3><%= person.nickname %></h3>
    <p><%= person.bio %></p>
    <ul>
      <li>
        <a href="<%= person.github_handle %>"><box-icon name='github' type='logo' color='#ffffff' size='md'></box-icon></a>
      </li>
      <li>
        <a href="<%= person.website %>"><box-icon name='world' color='#ffffff'size='md'></box-icon></a>
      </li>
    </ul>
  </div>
  <% }) %>
</section>

<%- include('./partials/foot') %>
